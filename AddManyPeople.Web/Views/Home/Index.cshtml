@model HomePageViewModel

<div b-ud2x8zyu5z="" class="container">
    <main b-ud2x8zyu5z="" role="main" class="pb-3">
        <div class="row">
            <div class="col-md-12">
                <a href="/home/add" class="btn btn-success" id="add">Add People</a>
                <table class="table table-hover table-bordered">
                    <thead>
                        <tr>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Age</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var p in Model.People)
                        {
                            <tr>
                                <td>@p.FirstName</td>
                                <td>@p.LastName</td>
                                <td>@p.Age</td>
                            </tr>
                        }

                    </tbody>
                </table>
            </div>
        </div>

    </main>
</div>